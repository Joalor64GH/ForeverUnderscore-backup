function loadAnimations()
{
    setTex('characters/base/BF_DEATH');

    addByPrefix('firstDeath', "BF dies", 24, false);
    addByPrefix('deathLoop', "BF Dead Loop", 24, true);
    addByPrefix('deathConfirm', "BF Dead confirm", 24, false);

    if (isPlayer)
        set('flipX', false);
    else
        set('flipX', true);
}
